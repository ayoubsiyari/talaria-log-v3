# API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_WEBSOCKET_URL=ws://localhost:5000

# Stripe Configuration (Public Keys Only)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME_CHAT=true
VITE_ENABLE_AFFILIATE_SYSTEM=true
VITE_ENABLE_FRAUD_DETECTION=true

# Environment
VITE_NODE_ENV=development

# Logging
VITE_LOG_LEVEL=info
VITE_ENABLE_CONSOLE_LOGS=true

# App Configuration
VITE_APP_NAME="Talaria Admin Dashboard"
VITE_APP_VERSION="1.0.0"
VITE_APP_DESCRIPTION="Advanced Administrative Interface for Trading Platform Management"

# Theme Configuration
VITE_DEFAULT_THEME=dark
VITE_ENABLE_THEME_SWITCHER=true

# Performance
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_CHUNK_SIZE_WARNING_LIMIT=1000

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_REDIRECT=false